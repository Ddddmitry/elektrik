{% extends 'marketplace.twig' %}

{% block notification %}
{% endblock %} {#notification#}
{% block search %}
    {% set search = {
        code: 'search',
        action: '?layout=marketplace',
        items: [{
            title: 'Услуга и специалист',
            name: 'servise_person',
            type: 'select',
            items: [{
                value: '1',
                title: 'Установка проводки',
                selected: 'selected'
            },{
                value: '2',
                title: 'Ремонт проводки на заводе'
            },{
                value: '3',
                title: 'Установка счётчиков'
            }]
        },{
            title: 'Метро район или город',
            name: 'place',
            type: 'select',
            items: [{
                value: '1',
                title: 'Алабинская',
                selected: 'selected'
            },{
                value: '2',
                title: 'Победа'
            },{
                value: '3',
                title: 'Безымянка'
            }]
        },{
            title: 'Поиск',
            name: 'submit',
            type: 'submit',
        }]
    } %}
    {% include './includes/search.twig' with {data: {
        size: 'xs',
        banner: {
            src: '/local/templates/.default/img/trash/banner2.jpg',
            name: 'Баннер2',
            size: {
                width: 1440,
                height: 400
            }
        },
        search: search,
    }} %}
{% endblock %} {#search#}
{% block breadcrumbs %}
    <div class="breadcrumbs">
        <div class="breadcrumbs-inner typicalBlock typicalBlock_two">
            <div class="breadcrumbs-list">
                <a href="/?layout=marketplace"
                   class="breadcrumbs-list__single">
                    {% include './includes/icon.twig' with {icon: {name: 'arrow-down', class: ''}} %}
                    <span class="breadcrumbs-list__single-text">К списку исполнителей</span>
                </a>
            </div>
        </div>
    </div>
{% endblock %} {#breadcrumbs#}
