    <script src="https://api-maps.yandex.ru/2.1/?lang=ru_RU" type="text/javascript"></script>
    <div id="on-map" class="fancyBlock">
        <div class="fancybox-title">Адрес</div>
        <div id="ymap" class="ymap"></div>
        <script>
            ymaps.ready(init);
            function init() {
                var myMap = new ymaps.Map("ymap", {
                        center: [{{ master.position.coords[0]|json_encode() }}, {{ master.position.coords[1]|json_encode() }}],
                        zoom: 12,
                        controls: ['zoomControl']
                    }, {
                        searchControlProvider: 'yandex#search'
                    });

                myMap.geoObjects
                    .add(new ymaps.Placemark([{{ master.position.coords[0]|json_encode() }}, {{ master.position.coords[1]|json_encode() }}], {
                        balloonContent: '{{ master.position.adress|json_encode() }}'
                    }, {
                        preset: 'islands#icon',
                        iconColor: '#d70926'
                    }));

            }

        </script>
    </div>

